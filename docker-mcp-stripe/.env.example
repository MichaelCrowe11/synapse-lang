# Stripe Configuration
# Copy this file to .env and fill in your actual values

# Stripe API Keys (get from https://dashboard.stripe.com/test/apikeys)
STRIPE_API_KEY=your_stripe_test_api_key_here
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret_here

# Server Configuration
MCP_SERVER_PORT=3002
CORS_ORIGINS=http://localhost:3000,http://localhost:8001,http://localhost:8000

# WARNING: Never commit actual API keys to version control!
# Always use environment variables or secure secret management