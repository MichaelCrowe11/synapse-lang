    "### Visualization with Uncertainty Bands"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "21",
   "metadata": {
    "language": "synapse"
   },
   "outputs": [],
   "source": [
    "%%synapse\n",
    "# Create visualization with uncertainty bands\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "# Extract data for plotting\n",
    "times = [r[\"time\"] for r in time_evolution_results]\n",
    "conc_A_values = [r[\"conc_A\"].value for r in time_evolution_results]\n",
    "conc_A_errors = [r[\"conc_A\"].uncertainty for r in time_evolution_results]\n",
    "conc_C_values = [r[\"conc_C\"].value for r in time_evolution_results]\n",
    "conc_C_errors = [r[\"conc_C\"].uncertainty for r in time_evolution_results]\n",
    "\n",
    "# Create the plot\n",
    "plt.figure(figsize=(10, 6))\n",
    "\n",
    "# Plot concentration A with uncertainty\n",
    "plt.errorbar(times, conc_A_values, yerr=conc_A_errors, \n",
    "             label='[A] (Reactant)', color='blue', alpha=0.7, capsize=3)\n",
    "plt.fill_between(times, \n",
    "                 np.array(conc_A_values) - np.array(conc_A_errors),\n",
    "                 np.array(conc_A_values) + np.array(conc_A_errors),\n",
    "                 alpha=0.2, color='blue')\n",
    "\n",
    "# Plot concentration C with uncertainty  \n",
    "plt.errorbar(times, conc_C_values, yerr=conc_C_errors,\n",
    "             label='[C] (Product)', color='red', alpha=0.7, capsize=3)\n",
    "plt.fill_between(times,\n",
    "                 np.array(conc_C_values) - np.array(conc_C_errors), \n",
    "                 np.array(conc_C_values) + np.array(conc_C_errors),\n",
    "                 alpha=0.2, color='red')\n",
    "\n",
    "plt.xlabel('Time (s)')\n",
    "plt.ylabel('Concentration (mol/L)')\n",
    "plt.title('Chemical Reaction Kinetics with Uncertainty\\nA + B â†’ C (Second-order reaction)')\n",
    "plt.legend()\n",
    "plt.grid(True, alpha=0.3)\n",
    "\n",
    "# Add text with key parameters\n",
    "plt.text(0.02, 0.98, \n",
    "         f'k = {rate_constant.value:.3f} Â± {rate_constant.uncertainty:.3f} L/mol/s\\n'\n",
    "         f'T = {temperature.value:.1f} Â± {temperature.uncertainty:.1f} K\\n'\n",
    "         f'Ea = {activation_energy.value/1000:.1f} Â± {activation_energy.uncertainty/1000:.1f} kJ/mol',\n",
    "         transform=plt.gca().transAxes, fontsize=10, \n",
    "         verticalalignment='top', bbox=dict(boxstyle='round', facecolor='white', alpha=0.8))\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "print(\"ðŸ“Š Plot created with uncertainty bands showing:\")\n",
    "print(\"   â€¢ Blue: Reactant concentration [A] with uncertainty\")\n",
    "print(\"   â€¢ Red: Product concentration [C] with uncertainty\")\n",
    "print(\"   â€¢ Shaded regions: Â±1Ïƒ confidence intervals\")"
   ]
