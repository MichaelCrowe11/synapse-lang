   "metadata": {},
   "source": [
    "### Parallel Parameter Sweeps\n",
    "\n",
    "Easily explore parameter spaces with uncertainty:"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "15",
   "metadata": {
    "language": synapse"
   },
   "outputs": [],
   "source": [
    "%%synapse\n",
    "# Parallel parameter sweep for projectile motion\n",
    "uncertain gravity = 9.81 Â± 0.01     # m/sÂ² (measurement uncertainty)\n",
    "uncertain air_resistance = 0.1 Â± 0.02  # drag coefficient uncertainty\n",
    "\n",
    "parallel parameter_sweep {\n",
    "    initial_velocity: [10, 20, 30, 40, 50]     # m/s\n",
    "    launch_angle: [15, 30, 45, 60, 75]         # degrees\n",
    "    \n",
    "    # Calculate range for each parameter combination\n",
    "    angle_rad = math.radians(launch_angle)\n",
    "    \n",
    "    # Simplified range formula (without air resistance)\n",
    "    range_no_air = (initial_velocity**2 * math.sin(2 * angle_rad)) / gravity\n",
    "    \n",
    "    # With air resistance (approximate)\n",
    "    range_with_air = range_no_air * (1 - air_resistance)\n",
    "    \n",
    "    emit {\n",
    "        \"velocity\": initial_velocity,\n",
    "        \"angle\": launch_angle, \n",
    "        \"range_no_air\": range_no_air,\n",
    "        \"range_with_air\": range_with_air\n",
    "    }\n",
    "}\n",
    "\n",
    "print(f\"Parameter sweep completed: {len(parameter_sweep_results)} combinations\")\n",
    "print(\"\\nSample results:\")\n",
    "for i, result in enumerate(parameter_sweep_results[:5]):\n",
    "    print(f\"  v={result['velocity']}m/s, Î¸={result['angle']}Â°: \"\n",
    "          f\"{result['range_with_air']:.1f}m\")"
   ]
