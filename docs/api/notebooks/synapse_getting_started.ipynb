   "source": [
    "## 5. ðŸ§ª Real-World Example: Chemical Reaction Analysis\n",
    "\n",
    "Let's put it all together with a comprehensive example analyzing chemical reaction kinetics:"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "19",
   "metadata": {
    "language": "synapse"
   },
   "outputs": [],
   "source": [
    "%%synapse\n",
    "import math\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "# Chemical reaction kinetics example\n",
    "# A + B â†’ C (second-order reaction)\n",
    "\n",
    "# Experimental conditions with uncertainty\n",
    "uncertain temperature = 298.15 Â± 0.5     # K\n",
    "uncertain initial_conc_A = 0.100 Â± 0.002  # mol/L\n",
    "uncertain initial_conc_B = 0.150 Â± 0.003  # mol/L\n",
    "\n",
    "# Rate constant with temperature dependence (Arrhenius equation)\n",
    "uncertain activation_energy = 75000 Â± 2000  # J/mol\n",
    "uncertain pre_exponential = 1.2e10 Â± 0.1e10  # L/mol/s\n",
    "R = 8.314  # J/mol/K (gas constant, assumed exact)\n",
    "\n",
    "# Calculate rate constant with uncertainty\n",
    "monte_carlo(samples=50000) {\n",
    "    rate_constant = pre_exponential * math.exp(-activation_energy / (R * temperature))\n",
    "}\n",
    "\n",
    "print(f\"Reaction Analysis Results:\")\n",
    "print(f\"Temperature: {temperature} K\")\n",
    "print(f\"Rate constant: {rate_constant:.3f} L/mol/s\")\n",
    "print(f\"Rate constant uncertainty: Â±{rate_constant.uncertainty:.3f} L/mol/s\")\n",
    "\n",
    "# Time evolution with parallel calculation\n",
    "time_points = np.linspace(0, 100, 21)  # 0 to 100 seconds\n",
    "\n",
    "parallel time_evolution {\n",
    "    time: time_points\n",
    "    \n",
    "    # Second-order kinetics: 1/[A] = 1/[A]â‚€ + kt\n",
    "    # Assuming [A]â‚€ = [B]â‚€ for simplicity\n",
    "    conc_A_inv = 1/initial_conc_A + rate_constant * time\n",
    "    conc_A = 1 / conc_A_inv\n",
    "    \n",
    "    # Concentration of product C\n",
    "    conc_C = initial_conc_A - conc_A\n",
    "    \n",
    "    emit {\n",
    "        \"time\": time,\n",
    "        \"conc_A\": conc_A,\n",
    "        \"conc_C\": conc_C\n",
    "    }\n",
    "}\n",
    "\n",
    "# Analysis of results\n",
    "half_life_time = None\n",
    "for result in time_evolution_results:\n",
    "    if result[\"conc_A\"].value <= initial_conc_A.value / 2:\n",
    "        half_life_time = result[\"time\"]\n",
    "        break\n",
    "\n",
    "print(f\"\\nKinetics Analysis:\")\n",
    "print(f\"Initial [A]: {initial_conc_A} mol/L\")\n",
    "print(f\"Half-life: ~{half_life_time:.1f} seconds\")\n",
    "\n",
    "# Calculate final concentration at t=100s\n",
    "final_result = time_evolution_results[-1]\n",
    "print(f\"Final [A] at 100s: {final_result['conc_A']:.4f} mol/L\")\n",
    "print(f\"Final [C] at 100s: {final_result['conc_C']:.4f} mol/L\")\n",
    "\n",
    "# Uncertainty analysis\n",
    "print(f\"\\nUncertainty Analysis:\")\n",
    "print(f\"Rate constant rel. unc.: {rate_constant.relative_uncertainty:.1%}\")\n",
    "print(f\"Final [A] rel. unc.: {final_result['conc_A'].relative_uncertainty:.1%}\")\n",
    "print(f\"Final [C] rel. unc.: {final_result['conc_C'].relative_uncertainty:.1%}\")"
   ]
  },
