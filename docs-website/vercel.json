{
  "version": 2,
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static_v2/(.*)",
      "dest": "/static_v2/$1"
    },
    {
      "src": "/(.*)",
      "dest": "index.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production"
  }
}
