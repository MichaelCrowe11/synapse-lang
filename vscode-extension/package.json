{
  "name": "synapse-lang",
  "displayName": "SynapseLang",
  "description": "Language support for SynapseLang - Drug Discovery DSL with molecular modeling, QSAR, and docking capabilities",
  "version": "0.1.0",
  "publisher": "synapse-lang",
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "activationEvents": [
    "onLanguage:synapse"
  ],
  "badges": [
    {
      "url": "https://img.shields.io/github/stars/MichaelCrowe11/synapse-lang?style=social",
      "href": "https://github.com/MichaelCrowe11/synapse-lang",
      "description": "Star on GitHub"
    }
  ],
  "categories": [
    "Programming Languages",
    "Snippets",
    "Linters",
    "Data Science"
  ],
  "keywords": [
    "drug-discovery",
    "cheminformatics",
    "molecular-modeling",
    "qsar",
    "docking",
    "pharmaceutical",
    "bioinformatics",
    "synapse"
  ],
  "main": "./extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "webpack --mode production",
    "watch": "webpack --mode development --watch",
    "test": "node ./dist/test/runTest.js",
    "package": "vsce package"
  },
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "synapse.runFile",
        "title": "Run Synapse File",
        "icon": "$(play)"
      },
      {
        "command": "synapse.checkSyntax",
        "title": "Check Syntax",
        "icon": "$(check)"
      },
      {
        "command": "synapse.generateQSAR",
        "title": "Generate QSAR Model",
        "icon": "$(graph)"
      },
      {
        "command": "synapse.runDocking",
        "title": "Run Molecular Docking",
        "icon": "$(rocket)"
      },
      {
        "command": "synapse.showHelp",
        "title": "Show SynapseLang Help",
        "icon": "$(question)"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "when": "resourceExtname == .syn",
          "command": "synapse.runFile",
          "group": "navigation"
        },
        {
          "when": "resourceExtname == .syn",
          "command": "synapse.checkSyntax",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "when": "resourceExtname == .syn",
          "command": "synapse.generateQSAR",
          "group": "synapse"
        },
        {
          "when": "resourceExtname == .syn",
          "command": "synapse.runDocking",
          "group": "synapse"
        }
      ]
    },
    "keybindings": [
      {
        "command": "synapse.run",
        "key": "f5",
        "when": "editorTextFocus && resourceExtname == .syn"
      },
      {
        "command": "synapse.format",
        "key": "shift+alt+f",
        "when": "editorTextFocus && resourceExtname == .syn"
      }
    ],
    "configuration": {
      "title": "Synapse",
      "properties": {
        "synapse.pythonPath": {
          "type": "string",
          "default": "python",
          "description": "Path to Python interpreter with SynapseLang installed"
        },
        "synapse.enableLinting": {
          "type": "boolean",
          "default": true,
          "description": "Enable syntax checking and linting"
        },
        "synapse.dockingEngine": {
          "type": "string",
          "enum": [
            "AutoDock Vina",
            "AutoDock 4",
            "Glide"
          ],
          "default": "AutoDock Vina",
          "description": "Default molecular docking engine"
        },
        "synapse.outputFormat": {
          "type": "string",
          "enum": [
            "json",
            "csv",
            "xlsx"
          ],
          "default": "json",
          "description": "Default output format for results"
        },
        "synapse.enablePharmKit": {
          "type": "boolean",
          "default": false,
          "description": "Enable PharmKit professional features (requires license)"
        }
      }
    },
    "languages": [
      {
        "id": "synapse",
        "aliases": [
          "Synapse",
          "synapse"
        ],
        "extensions": [
          ".syn",
          ".synapse"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "synapse",
        "scopeName": "source.synapse",
        "path": "./syntaxes/synapse.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "synapse",
        "path": "./snippets/synapse.json"
      }
    ],
    "themes": [
      {
        "label": "Synapse Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/synapse-dark.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichaelCrowe11/synapse-lang.git"
  },
  "bugs": {
    "url": "https://github.com/MichaelCrowe11/synapse-lang/issues"
  },
  "homepage": "https://github.com/MichaelCrowe11/synapse-lang#readme",
  "license": "SEE LICENSE IN LICENSE-DUAL.md",
  "author": "",
  "dependencies": {
    "vscode-languageclient": "^9.0.1",
    "vscode-languageserver": "^9.0.1",
    "vscode-languageserver-textdocument": "^1.0.12"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "@types/vscode": "^1.104.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.2",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1"
  }
}
