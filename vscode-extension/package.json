{
  "name": "synapse-lang",
  "displayName": "SynapseLang",
  "description": "Language support for SynapseLang - Quantum computing and scientific reasoning language with parallel thought processing",
  "version": "0.1.0",
  "publisher": "synapse-lang",
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "activationEvents": [
    "onLanguage:synapse"
  ],
  "badges": [
    {
      "url": "https://img.shields.io/github/stars/MichaelCrowe11/synapse-lang?style=social",
      "href": "https://github.com/MichaelCrowe11/synapse-lang",
      "description": "Star on GitHub"
    }
  ],
  "categories": [
    "Programming Languages",
    "Snippets",
    "Linters",
    "Data Science"
  ],
  "keywords": [
    "quantum-computing",
    "quantum-algorithms",
    "scientific-computing",
    "parallel-processing",
    "uncertainty-quantification",
    "reasoning-chains",
    "tensor-operations",
    "synapse"
  ],
  "main": "./extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "webpack --mode production",
    "watch": "webpack --mode development --watch",
    "test": "node ./dist/test/runTest.js",
    "package": "vsce package"
  },
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "synapse.runFile",
        "title": "Run Synapse File",
        "icon": "$(play)"
      },
      {
        "command": "synapse.checkSyntax",
        "title": "Check Syntax",
        "icon": "$(check)"
      },
      {
        "command": "synapse.runQuantumCircuit",
        "title": "Run Quantum Circuit",
        "icon": "$(circuit-board)"
      },
      {
        "command": "synapse.visualizeQuantumState",
        "title": "Visualize Quantum State",
        "icon": "$(graph)"
      },
      {
        "command": "synapse.runParallelExecution",
        "title": "Run Parallel Branches",
        "icon": "$(split-horizontal)"
      },
      {
        "command": "synapse.analyzeUncertainty",
        "title": "Analyze Uncertainty Propagation",
        "icon": "$(warning)"
      },
      {
        "command": "synapse.debugReasoningChain",
        "title": "Debug Reasoning Chain",
        "icon": "$(debug)"
      },
      {
        "command": "synapse.showQuantumHelp",
        "title": "Show Quantum Computing Help",
        "icon": "$(question)"
      },
      {
        "command": "synapse.openREPL",
        "title": "Open Synapse REPL",
        "icon": "$(terminal)"
      },
      {
        "command": "synapse.formatDocument",
        "title": "Format Synapse Document",
        "icon": "$(code)"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "when": "resourceExtname == .syn",
          "command": "synapse.runFile",
          "group": "navigation"
        },
        {
          "when": "resourceExtname == .syn",
          "command": "synapse.checkSyntax",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "when": "resourceExtname == .syn",
          "command": "synapse.runQuantumCircuit",
          "group": "synapse"
        },
        {
          "when": "resourceExtname == .syn",
          "command": "synapse.visualizeQuantumState",
          "group": "synapse"
        },
        {
          "when": "resourceExtname == .syn",
          "command": "synapse.runParallelExecution",
          "group": "synapse"
        }
      ]
    },
    "keybindings": [
      {
        "command": "synapse.run",
        "key": "f5",
        "when": "editorTextFocus && resourceExtname == .syn"
      },
      {
        "command": "synapse.format",
        "key": "shift+alt+f",
        "when": "editorTextFocus && resourceExtname == .syn"
      }
    ],
    "configuration": {
      "title": "Synapse",
      "properties": {
        "synapse.pythonPath": {
          "type": "string",
          "default": "python",
          "description": "Path to Python interpreter with SynapseLang installed"
        },
        "synapse.enableLinting": {
          "type": "boolean",
          "default": true,
          "description": "Enable syntax checking and linting"
        },
        "synapse.quantumBackend": {
          "type": "string",
          "enum": [
            "synapse_simulator",
            "qiskit",
            "cirq",
            "pennylane"
          ],
          "default": "synapse_simulator",
          "description": "Quantum computing backend for circuit execution"
        },
        "synapse.maxQubits": {
          "type": "number",
          "default": 24,
          "minimum": 1,
          "maximum": 30,
          "description": "Maximum number of qubits for simulation"
        },
        "synapse.enableQuantumNoise": {
          "type": "boolean",
          "default": false,
          "description": "Enable quantum noise models in simulations"
        },
        "synapse.parallelThreads": {
          "type": "number",
          "default": 4,
          "minimum": 1,
          "maximum": 16,
          "description": "Number of threads for parallel execution"
        },
        "synapse.outputFormat": {
          "type": "string",
          "enum": [
            "json",
            "csv",
            "xlsx"
          ],
          "default": "json",
          "description": "Default output format for results"
        },
        "synapse.enableAdvancedFeatures": {
          "type": "boolean",
          "default": false,
          "description": "Enable advanced quantum algorithms and tensor operations"
        },
        "synapse.uncertaintyPrecision": {
          "type": "number",
          "default": 6,
          "minimum": 1,
          "maximum": 15,
          "description": "Decimal precision for uncertainty calculations"
        },
        "synapse.debugMode": {
          "type": "boolean",
          "default": false,
          "description": "Enable debug mode with detailed execution traces"
        }
      }
    },
    "languages": [
      {
        "id": "synapse",
        "aliases": [
          "Synapse",
          "synapse"
        ],
        "extensions": [
          ".syn",
          ".synapse"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "synapse",
        "scopeName": "source.synapse",
        "path": "./syntaxes/synapse.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "synapse",
        "path": "./snippets/synapse.json"
      }
    ],
    "themes": [
      {
        "label": "Synapse Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/synapse-dark.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichaelCrowe11/synapse-lang.git"
  },
  "bugs": {
    "url": "https://github.com/MichaelCrowe11/synapse-lang/issues"
  },
  "homepage": "https://github.com/MichaelCrowe11/synapse-lang#readme",
  "license": "SEE LICENSE IN LICENSE-DUAL.md",
  "author": "",
  "dependencies": {
    "vscode-languageclient": "^9.0.1",
    "vscode-languageserver": "^9.0.1",
    "vscode-languageserver-textdocument": "^1.0.12"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@types/vscode": "^1.105.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1"
  }
}
